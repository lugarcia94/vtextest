#yotubeplay
    position: absolute 
    top: 0
    left: 0 
    z-index: 10    
    width: 100% 
    height: 100% 
    opacity:0 
    visibility: hidden 
    transition: all .2s ease-in-out
    iframe 
        width: 100% !important 
        height: 100% !important
    .on-play &
        opacity: 1 
        visibility: visible 


#show
    position                : relative    

#divTitulo
