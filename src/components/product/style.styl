                    padding-bottom: 50px
                    max-height: 130px 
                    overflow: hidden!important
                    position: relative
                    transition: all 150ms ease-in-out
                    &:before
                        content         : 'Continuar lendo'
                        color           : color[0]
                        background      : #fff
                        height          : 50px
                        position        : absolute
                        bottom          : 0 
                        width           : 100% 
                        display         : flex
                        align-items     : center
                        justify-content : flex-start
                        font-size       : 12px
